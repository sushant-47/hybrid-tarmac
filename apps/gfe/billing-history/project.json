{
    "name": "billing-history",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "prefix": "bh",
    "sourceRoot": "apps/gfe/billing-history/src",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@angular/build:application",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/{projectRoot}",
                "browser": "{projectRoot}/src/main.ts",
                "tsConfig": "{projectRoot}/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    {
                        "glob": "**/*",
                        "input": "{projectRoot}/public"
                    }
                ],
                "styles": ["{projectRoot}/src/styles.scss"],
                "stylePreprocessorOptions": {
                    "includePaths": ["{projectRoot}/src/app/styles"]
                }
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "4kb",
                            "maximumError": "8kb"
                        }
                    ],
                    "outputHashing": "all"
                },
                "development": {
                    "optimization": false,
                    "extractLicenses": false,
                    "sourceMap": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "continuous": true,
            "executor": "@angular/build:dev-server",
            "configurations": {
                "production": {
                    "buildTarget": "billing-history:build:production"
                },
                "development": {
                    "buildTarget": "billing-history:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "extract-i18n": {
            "executor": "@angular/build:extract-i18n",
            "options": {
                "buildTarget": "billing-history:build"
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "apps/gfe/billing-history/jest.config.ts",
                "tsConfig": "{projectRoot}/tsconfig.spec.json"
            }
        },
        "serve-static": {
            "continuous": true,
            "executor": "@nx/web:file-server",
            "options": {
                "buildTarget": "billing-history:build",
                "staticFilePath": "dist/apps/gfe/billing-history/browser",
                "spa": true
            }
        }
    }
}
